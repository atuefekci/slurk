    "css": {
        "#image-area": {
            "display": "none"
        },
        "header, footer": {
            "background": "#115E91"
        },
        "header": {
            "border-bottom": "#115E91 solid 1px",
            "z-index": "10"
        },
        "h1": {
            "padding-top": "20px"
        },
        "#sidebar": {
            "overflow-y": "scroll",
            "overflow-x": "scroll"
        },
        "#show-area": {
            "display": "table",
            "margin-left": "30px",
            "margin-top": "30px",
            "padding": "30px",
            "background-color": "rgb(182, 226, 226)"
        },
        "#question": {
            "text-align": "left",
            "padding": "20px",
            "font-size": "20px"
        },
        ".caption": {
            "padding-top": "10px",
            "padding-bottom": "30px",
            "text-align": "center"
        },
        ".not_zoomed": {
            "border-radius": "5px",
            "cursor": "pointer"
        },
        ".zoom": {
            "display": "none",
            "background-color": "rgba(0,0,0,0.6)",
            "position": "absolute",
            "margin-top": "30px",
            "margin-left": "30px"
        },
        ".original_img": {
            "text-align": "center",
            "margin-top": "100px"
        },
        ".textline": {
            "text-align": "center",
            "font-size": "20px",
            "padding-bottom": "10px"
        },
        "header .button": {
            "top": "45px",
            "right": "2px",
            "position": "absolute"
        },
        ".button": {
            "display": "inline-block"
        },
        ".button_content": {
            "visibility": "hidden",
            "background-color": "black",
            "font-size": "15px",
            "color": "#fff",
            "width": "500px",
            "text-align": "center",
            "padding": "20px",
            "border-radius": "6px",
            "position": "absolute",
            "top": "-5px",
            "right": "105%"
        },
        ".button:hover .button_content": {
            "visibility": "visible"
        }
    },
    "scripts": {
        "incoming-message": "display-message",
        "submit-message": "send-message",
        "print-history": "plain-history",
        "plain": "cola-script"
    }
}
